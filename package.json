{
  "name": "currency-trader-backend",
  "version": "1.0.0",
  "description": "Backend APIs for currency trader platform",
  "directories": {
    "test": "test"
  },
  "nodemonConfig": {
    "watch": [
      "./"
    ],
    "ext": "ts",
    "ignore": [
      "*.test.ts"
    ],
    "execMap": {
      "ts": "ts-node"
    }
  },
  "scripts": {
    "dev": "nodemon index.ts",
    "install": "npm install",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/amasij/currency-trader-backend.git"
  },
  "author": "Simon Joseph",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/amasij/currency-trader-backend/issues"
  },
  "homepage": "https://github.com/amasij/currency-trader-backend#readme",
  "dependencies": {
    "express": "^4.17.3"
  },
  "devDependencies": {
    "@types/express": "^4.17.13",
    "@types/node": "^17.0.21",
    "ts-node": "^10.6.0",
    "typescript": "^4.6.2"
  }
}
